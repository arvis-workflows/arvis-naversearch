{
  "$schema": "https://raw.githubusercontent.com/jopemachine/arvis-extension-validator/master/workflow-schema.json",
  "creator": "Jinuk Baek",
  "description": "Naver Search Kit",
  "name": "Naver Search",
  "readme": "Autocomplete Workflow for Naver Services",
  "version": "0.0.1",
  "webAddress": "https://www.jinukbaek.com/blog/ko/archives/216",
  "enabled": true,
  "defaultIcon": "icon.png",
  "platform": [
    "darwin",
    "linux"
  ],
  "commands": [
    {
      "type": "scriptFilter",
      "command": "na",
      "title": "Search Naver for {query}",
      "subtitle": "",
      "scriptFilter": "python naver_search.py \"{query}\"",
      "runningSubtext": "Running",
      "withspace": true,
      "argType": "required",
      "actions": [
        {
          "modifiers": "normal",
          "type": "open",
          "target": "http://search.naver.com/search.naver?ie=utf8&sm=stp_hty&where=se&query={query}"
        }
      ]
    },
    {
      "type": "scriptFilter",
